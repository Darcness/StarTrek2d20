{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "es2015.collection",
      "es5",
      "es2015.promise"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks" :false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
